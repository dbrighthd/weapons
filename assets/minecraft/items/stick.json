{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when": [
            "Darksaber Hilt",
            "The Darksaber's Hilt",
            "iregex:.*darksaber.*"
          ],
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/.weapons/lightsabers/dark/darksaber_hilt"
          }
        },
        {
          "when": ["Double Lightsaber Hilt", "iregex:.*double.*lightsaber.*hilt.*"],
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/.weapons/lightsabers/doublelightsaberhilt"
          }
        },
        {
          "when": ["Crossguard Lightsaber Hilt", "iregex:.*crossguard.*lightsaber.*hilt.*"],
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/.weapons/lightsabers/xguardlightsaberhilt"
          }
        },
        {
          "when": ["Lightsaber Hilt", "iregex:.*lightsaber.*hilt.*"],
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/.weapons/lightsabers/lightsaberhilt"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/stick"
      }
    }
  }