{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when":["Copper Scimitar", "iregex:.*scimitar.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/scimitars/copper_scimitar"
          }
        },
        {
          "when":["Copper Comically Large Butter Knife", "Comically Large Butter Knife", "iregex:.*Large.*Butter.*Knife.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/comically_large_utensils/copper_comically_large_butter_knife"
          }
        },
      {
        "when": [
          "Copper Dancer's Sword",
          "iregex:.*Dancer.*s.*Sword.*"
        ],
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/.weapons/dancers_swords/copper_dancers_sword"
        }
      },
      {
        "when": [
          "Copper Soul Knife",
          "iregex:.*Soul.*Knife.*"
        ],
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/.weapons/soul_knives/copper_soul_knife"
        }
      },
      {
        "when":["Copper BFS", "iregex:.*BFS.*"],
        "model": {
          "type":"minecraft:model",
          "model":"minecraft:item/.weapons/buster_swords/busterswordc"
        }
      },
        {
          "when": ["Green Flaming Copper Sword", "iregex:.*green.*flaming.*"],
          "model": {
            "type": "minecraft:condition",
            "property": "minecraft:component",
            "predicate": "minecraft:enchantments",
            "value": [
              {
                "enchantments": "minecraft:fire_aspect"
              }
            ],
            "on_true": {
              "type": "minecraft:model",
              "model": "minecraft:item/.weapons/flaming_swords/green_flaming_copper_sword"
            },
            "on_false": {
              "type": "minecraft:model",
              "model": "minecraft:item/copper_sword"
            }
          }
        },
        {
          "when": ["Flaming Copper Sword", "iregex:.*flaming.*"],
          "model": {
            "type": "minecraft:condition",
            "property": "minecraft:component",
            "predicate": "minecraft:enchantments",
            "value": [
              {
                "enchantments": "minecraft:fire_aspect"
              }
            ],
            "on_true": {
              "type": "minecraft:model",
              "model": "minecraft:item/.weapons/flaming_swords/flaming_copper_sword"
            },
            "on_false": {
              "type": "minecraft:model",
              "model": "minecraft:item/copper_sword"
            }
          }
        },
        {
          "when":["Copper Broadsword", "iregex:.*broadsword.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/broadswords/broadswordc"
          }
        },
        {
          "when":["Copper Claymore", "iregex:.*claymore.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/claymores/claymorec"
          }
        },
        {
          "when":["Copper Cutlass", "iregex:.*cutlass.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/cutlasses/cutlassc"
          }
        },
        {
          "when":["Copper Katana", "iregex:.*katana.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/katanas/katanac"
          }
        },
        {
          "when":["Oxidized Copper Sword", "iregex:.*oxidized.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/copper_set/c/oxidized/oxidized_copper_sword"
          }
        },
        {
          "when":["Weathered Copper Sword", "iregex:.*weathered.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/copper_set/c/weathered/weathered_copper_sword"
          }
        },
        {
          "when":["Exposed Copper Sword", "iregex:.*exposed.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/copper_set/c/exposed/exposed_copper_sword"
          }
        },
        {
          "when":["Waxed Copper Sword", "iregex:.*waxed.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/copper_sword"
          }
        },
        {
          "when": ["Copper Sword", "iregex:.*copper.*"],
          "model": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:damage",
            "scale": 5,
            "entries": [
              {
                "threshold": 1,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/copper_sword"
                }
              },
              {
                "threshold": 2,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/copper_set/c/exposed/exposed_copper_sword"
                }
              },
              {
                "threshold": 3,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/copper_set/c/weathered/weathered_copper_sword"
                }
              },
              {
                "threshold": 4,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/copper_set/c/oxidized/oxidized_copper_sword"
                }
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/copper_sword"
            }
          }
        }
      ],
      "fallback": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:damage",
            "scale": 5,
            "entries": [
              {
                "threshold": 1,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/copper_sword"
                }
              },
              {
                "threshold": 2,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/copper_set/c/exposed/exposed_copper_sword"
                }
              },
              {
                "threshold": 3,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/copper_set/c/weathered/weathered_copper_sword"
                }
              },
              {
                "threshold": 4,
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/copper_set/c/oxidized/oxidized_copper_sword"
                }
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/copper_sword"
            }
      }
    }
  }