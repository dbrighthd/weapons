        {
          "when":["Diamond Gold Axe-olotl", "iregex:.*gold.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/d/goldd"
          }
        },
        {
          "when":["Diamond Lucy Axe-olotl", "iregex:.*lucy.*axe-olotl.*", "Diamond Axe-olotl", "iregex:.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/d/lucyd"
