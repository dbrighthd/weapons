{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when":["Diamond Halberd", "iregex:.*halberd.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/halberds/diamond_halberd"
          }
        },
        {
          "when":["Ruby Axe", "iregex:.*ruby.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/ruby_set/items/diamond/ruby_axe"
          }
        },
        {
          "when":["Emerald Axe", "iregex:.*emerald.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/emerald_set/items/diamond/emerald_axe"
          }
        },
        {
          "when":["Amethyst Axe", "iregex:.*amethyst.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/amethyst_set/items/diamond/amethyst_axe"
          }
        },
        {
          "when":["Diamond Blue Axe-olotl", "iregex:.*blue.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/d/blued"
          }
        },
        {
          "when":["Diamond Cyan Axe-olotl", "iregex:.*cyan.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/d/cyand"
          }
        },
        {
          "when":["Diamond Wild Axe-olotl", "iregex:.*wild.*axe-olotl.*"],
          "model": {
