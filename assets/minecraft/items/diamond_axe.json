          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/dungeons_axes/dungeons_axed"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/diamond_axe"
      }
    }
  }