{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when":["Copper Halberd", "iregex:.*halberd.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/halberds/copper_halberd"
          }
        },
        {
          "when":["Copper Blue Axe-olotl", "iregex:.*blue.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/c/bluec"
          }
        },
        {
          "when":["Copper Cyan Axe-olotl", "iregex:.*cyan.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/c/cyanc"
          }
        },
        {
          "when":["Copper Wild Axe-olotl", "iregex:.*wild.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/c/wildc"
          }
        },
        {
          "when":["Copper Gold Axe-olotl", "iregex:.*gold.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/c/goldc"
          }
        },
        {
          "when":["Copper Lucy Axe-olotl", "iregex:.*lucy.*axe-olotl.*", "Copper Axe-olotl", "iregex:.*axe-olotl.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/axe-olotls/c/lucyc"
          }
        },
        {
          "when":["Copper Double Axe", "iregex:.*double.*axe.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/double_axes/double_axec"
          }
        },
        {
          "when":["Copper Dungeons Axe", "iregex:.*dungeons.*axe.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/dungeons_axes/dungeons_axec"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/copper_axe"
      }
    }
  }