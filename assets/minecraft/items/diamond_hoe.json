{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when":["Obsidian Hoe", "iregex:.*obsidian.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/obsidian_set/items/diamond/obsidian_hoe"
          }
        },
        {
          "when":["Ruby Hoe", "iregex:.*ruby.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/ruby_set/items/diamond/ruby_hoe"
          }
        },
        {
          "when":["Emerald Hoe", "iregex:.*emerald.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/emerald_set/items/diamond/emerald_hoe"
          }
        },
        {
          "when":["Amethyst Hoe", "iregex:.*amethyst.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/amethyst_set/items/diamond/amethyst_hoe"
          }
        },
        {
          "when": ["Ruby Scythe", "iregex:.*ruby.*scythe.*"],
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:display_context",
            "cases": [
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/scythes/ruby_scythe"
                },
                "when": [
                  "gui",
                  "ground",
                  "fixed",
                  "on_shelf"
                ]
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/.weapons/scythes/ruby_scythe_in_hand"
            }
          }
        },
        {
          "when": ["Emerald Scythe", "iregex:.*emerald.*scythe.*"],
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:display_context",
            "cases": [
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/scythes/emerald_scythe"
                },
                "when": [
                  "gui",
                  "ground",
                  "fixed",
                  "on_shelf"
                ]
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/.weapons/scythes/emerald_scythe_in_hand"
            }
          }
        },
        {
          "when": ["Amethyst Scythe", "iregex:.*amethyst.*scythe.*"],
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:display_context",
            "cases": [
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/scythes/amethyst_scythe"
                },
                "when": [
                  "gui",
                  "ground",
                  "fixed",
                  "on_shelf"
                ]
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/.weapons/scythes/amethyst_scythe_in_hand"
            }
          }
        },
        {
          "when": ["Diamond Scythe", "iregex:.*scythe.*"],
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:display_context",
            "cases": [
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "minecraft:item/.weapons/scythes/diamond_scythe"
                },
                "when": [
                  "gui",
                  "ground",
                  "fixed",
                  "on_shelf"
                ]
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "minecraft:item/.weapons/scythes/diamond_scythe_in_hand"
            }
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/diamond_hoe"
      }
    }
  }