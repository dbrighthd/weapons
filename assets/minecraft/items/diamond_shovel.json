{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when":["Ruby Comically Large Spoon", "iregex:.*Ruby.*Large.*Spoon.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/comically_large_utensils/ruby_comically_large_spoon"
          }
        },
        {
          "when":["Emerald Comically Large Spoon", "iregex:.*Emerald.*Large.*Spoon.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/comically_large_utensils/emerald_comically_large_spoon"
          }
        },
        {
          "when":["Amethyst Comically Large Spoon", "iregex:.*Amethyst.*Large.*Spoon.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/comically_large_utensils/amethyst_comically_large_spoon"
          }
        },
        {
          "when":["Diamond Comically Large Spoon", "Comically Large Spoon", "iregex:.*Large.*Spoon.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/comically_large_utensils/diamond_comically_large_spoon"
          }
        },
        {
          "when":["Ruby Shovel", "iregex:.*ruby.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/ruby_set/items/diamond/ruby_shovel"
          }
        },
        {
          "when":["Emerald Shovel", "iregex:.*emerald.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/emerald_set/items/diamond/emerald_shovel"
          }
        },
        {
          "when":["Amethyst Shovel", "iregex:.*amethyst.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/amethyst_set/items/diamond/amethyst_shovel"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/diamond_shovel"
      }
    }
  }