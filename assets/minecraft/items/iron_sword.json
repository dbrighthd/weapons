{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when":["Iron Truthseeker", "iregex:.*truthseeker.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/truthseekers/truthseekeri"
          }
        },
        {
          "when": ["Soul Scythe", "iregex:.*soul.*scythe.*"],
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/.weapons/scythes/sword/_misc_scythes/soul_scythe"
          }
        },
        {
          "when": ["Jailor's Scythe", "iregex:.*jailor.*s.*scythe.*"],
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/.weapons/scythes/sword/_misc_scythes/jailors_scythe"
          }
        },
        {
          "when": ["Iron Scythe", "iregex:.*scythe.*"],
          "model": {
    "type": "minecraft:select",
    "property": "minecraft:display_context",
    "cases": [
      {
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/.weapons/scythes/iron_scythe"
        },
        "when": [
          "gui",
          "ground",
          "fixed",
          "on_shelf"
        ]
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/.weapons/scythes/sword/sword/iron_scythe_in_hand"
    }
          }
        },
        {
          "when":["Iron Scimitar", "iregex:.*scimitar.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/scimitars/iron_scimitar"
          }
        },
        {
          "when":["Iron Comically Large Butter Knife", "Comically Large Butter Knife", "iregex:.*Large.*Butter.*Knife.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/comically_large_utensils/iron_comically_large_butter_knife"
          }
        },
      {
        "when": [
          "Iron Dancer's Sword",
          "iregex:.*Dancer.*s.*Sword.*"
        ],
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/.weapons/dancers_swords/iron_dancers_sword"
        }
      },
      {
        "when": [
          "Iron Soul Knife",
          "iregex:.*Soul.*Knife.*"
        ],
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/.weapons/soul_knives/iron_soul_knife"
        }
      },
      {
        "when":["Iron BFS", "iregex:.*BFS.*"],
        "model": {
          "type":"minecraft:model",
          "model":"minecraft:item/.weapons/buster_swords/busterswordi"
        }
      },
        {
          "when":["Sting", "iregex:.*sting.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/misc/sting/sting"
          }
        },
        {
          "when":["Elven Blade", "iregex:.*elven.*blade.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/misc/elven_blade/elven_blade"
          }
        },
        {
          "when": ["Flaming Iron Sword", "iregex:.*flaming.*"],
          "model": {
            "type": "minecraft:condition",
            "property": "minecraft:component",
            "predicate": "minecraft:enchantments",
            "value": [
              {
                "enchantments": "minecraft:fire_aspect"
              }
            ],
            "on_true": {
              "type": "minecraft:model",
              "model": "minecraft:item/.weapons/flaming_swords/flaming_iron_sword"
            },
            "on_false": {
              "type": "minecraft:model",
              "model": "minecraft:item/iron_sword"
            }
          }
        },
        {
          "when":["Iron Broadsword", "iregex:.*broadsword.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/broadswords/broadswordi"
          }
        },
        {
          "when":["Iron Claymore", "iregex:.*claymore.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/claymores/claymorei"
          }
        },
        {
          "when":["Iron Cutlass", "iregex:.*cutlass.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/cutlasses/cutlassi"
          }
        },
        {
          "when":["Iron Katana", "iregex:.*katana.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/katanas/katanai"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/iron_sword"
      }
    }
  }