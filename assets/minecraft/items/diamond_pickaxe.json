{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:component",
    "component": "minecraft:custom_name",
    "cases": [
        {
          "when":["Sapphire Pickaxe", "iregex:.*sapphire.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/sapphire_set/diamond/sapphire_pickaxe"
          }
        },
        {
          "when":["Obsidian Pickaxe", "iregex:.*obsidian.*"],
          "model": {
            "type":"minecraft:model",
            "model":"minecraft:item/.weapons/obsidian_set/diamond/obsidian_pickaxe"
          }
        },
      {
        "when":["Ruby Pickaxe", "iregex:.*ruby.*"],
        "model": {
          "type":"minecraft:model",
          "model":"minecraft:item/.weapons/ruby_set/items/diamond/ruby_pickaxe"
        }
      },
      {
        "when":["Emerald Pickaxe", "iregex:.*emerald.*"],
        "model": {
          "type":"minecraft:model",
          "model":"minecraft:item/.weapons/emerald_set/items/diamond/emerald_pickaxe"
        }
      },
      {
        "when":["Amethyst Pickaxe", "iregex:.*amethyst.*"],
        "model": {
          "type":"minecraft:model",
          "model":"minecraft:item/.weapons/amethyst_set/items/diamond/amethyst_pickaxe"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/diamond_pickaxe"
    }
  }
}